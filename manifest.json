{
  "manifest_version": 3,
  "name": "Proxy Extension",
  "version": "2.0",
  "description": "Advanced proxy extension with SOCKS5 support and dark theme.",
  "permissions": [
    "proxy",
    "activeTab",
    "storage"
  ],
  "web_accessible_resources": [
    {
      "resources": ["icons/frontend/*.svg"],
      "matches": ["<all_urls>"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Proxy Extension (Disconnected)",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}